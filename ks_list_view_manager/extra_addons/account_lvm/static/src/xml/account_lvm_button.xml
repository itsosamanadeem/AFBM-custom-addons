         <xpath expr="//div[hasclass('o_list_buttons')]" position="inside">
            <t t-call="ks_ListView.buttons"/>
        </xpath>
     </t>
    <t t-name="ks_list_view_manager.hr_expense_button" t-inherit="hr_expense.ListButtons" t-inherit-mode="extension"  owl="1">
